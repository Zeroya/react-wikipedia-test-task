{
  "arrowParens": "always",
  "bracketSpacing": false,
  "tsxBracketSameLine": true,
  "tsxSingleQuote": true,
  "printWidth": 200,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "endOfLine": "auto",
  "jsxBracketSameLine": true
}
